local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

-- GUI Window
local Window = OrionLib:MakeWindow({Name = "Daily Chest Claimer", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionConfig"})

-- GUI Tab
local Tab = Window:MakeTab({
    Name = "Chest Services",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Function to claim the chest
local function claimChest()
    local chestService = game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index")
        :WaitForChild("sleitnick_knit@1.5.1"):WaitForChild("knit")
        :WaitForChild("Services"):WaitForChild("ChestService")
        :WaitForChild("RF"):WaitForChild("ClaimDailyChest")

    -- Try to invoke the server for claiming the chest
    local success, err = pcall(function()
        chestService:InvokeServer()
    end)

    -- Check for errors
    if not success then
        warn("Error claiming chest: " .. err)
    end
end

-- Loop to invoke the server every X seconds
local function startLoop(interval)
    while true do
        claimChest()
        wait(interval)  -- Loop interval, can be adjusted
    end
end

-- Button to start the loop
Tab:AddButton({
    Name = "Start Chest Claim Loop",
    Callback = function()
        startLoop(0.1)  -- Interval set to 60 seconds
    end
})

-- Initialize the GUI
OrionLib:Init()

